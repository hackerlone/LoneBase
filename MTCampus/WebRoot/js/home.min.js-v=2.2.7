$(function(){
    /*add nancy */
    /*弹出登录的弹窗，点击微博或者QQ登录时，关闭当前弹窗*/
    $('#icon_qq ,#icon_wb').click(function(e){
        $.colorbox.close();
        parent.jQuery.colorbox.close();
    })
    $("#sidebar .menu_box").each(function(i){
        $(this).hoverDelay({
            hoverDuring: 200,
            hoverEvent: function(){
                switch (i){
                    /*case 1:
                        $(this).addClass('current').children('.menu_sub').css({top:$(this).prev().position().top + 120}).removeClass('dn');
                        break;
                    case 2:
                        $(this).addClass('current').children('.menu_sub').css({top:$(this).prev().position().top }).removeClass('dn');
                        break;
                    case 3:
                        $(this).addClass('current').children('.menu_sub').css({top:$(this).position().top + $(this).height() - $(this).children('.menu_sub').height() +64}).removeClass('dn');
                        break;
                    case 4:
                        $(this).addClass('current').children('.menu_sub').css({top:$(this).position().top + $(this).height() - $(this).children('.menu_sub').height() +24}).removeClass('dn');
                        break;
                    case 5:
                        $(this).addClass('current').children('.menu_sub').css({top:$(this).position().top + $(this).height() - $(this).children('.menu_sub').height() -24}).removeClass('dn');
                        break;*/
                    default:
                        $(this).addClass('current').children('.menu_sub').css({top:$(this).position().top}).removeClass('dn');
                }
            },
            outEvent: function(){
                $(this).removeClass('current').children('.menu_sub').addClass('dn');
            }
        })
    });

    var winWidth = 0;
    var winHeight = 0;
    function findDimensions(){
        if (window.innerHeight)
            winHeight = window.innerHeight;
        else if ((document.body) && (document.body.clientHeight))
            winHeight = document.body.clientHeight;
        //通过深入Document内部对body进行检测，获取窗口大小
        if (document.documentElement  && document.documentElement.clientHeight)
        {
            winHeight = document.documentElement.clientHeight;
        }
        return winHeight;
    }

    //tab effects
    $('.hotabbing li').bind('click',function(){
        var index = $(this).index();
        if(index<2){
            $(this).addClass('current').siblings().removeClass('current');
            $('#hotList ul.hot_pos').each(function(i){
                $(this).fadeOut(200);
            });
            $('#hotList ul.hot_pos').eq(index).fadeIn(200);
        }
    });

    //要过期职位提示
    $('.tipExpire .expire_close').bind('click',function(){
        var count = $.cookie('showExpriedIndex');
        $.cookie('showExpriedIndex',count-1);
        $(this).parent('.tipExpire').hide();
    });

    //简历分数
    $('#resumeScore .xScore').bind('click',function(){
        $(this).parent('#resumeScore').hide();
        $.ajax({
            type:'POST',
            data: {},
            url:ctx+'/resume/clearShowNoticeInSeesion.json'
        }).done(function(result) {
        });
    });
    /*** main banner effect IN HOMEPAGE  ***/
    var bannerNum = $('.banner_bg li').length;
    var i=1;
    $('.banner_bg').css('height',160*bannerNum+'px');
    //rotate function
    function bannerGalleryLoop(index){
        if(index>=0){
            i = index;
        }

        $('.banner_control em').animate({"top": 55*i+"px"},200);
        $(".thumbs li").removeClass("current");
        $(".thumbs li").eq(i).addClass("current");
        $(".banner_bg").animate({"margin-top": "-" +160*i + "px" },200);

        i++;
        if(i>=bannerNum){
            i=0;
        }
    }

    //start auto rotate
    var bannerGalleryInterval = setInterval(bannerGalleryLoop,5000);

    //thumbs events
    $(".thumbs li").hover(function(){
        if($(this).hasClass("current")){return;};
        $(".banner_bg").stop(false,true,true);
        $(".thumbs li").stop(false,true,true);
        $(".banner_control em").stop(false,true,true);
        clearInterval(bannerGalleryInterval);

        var currentIndex =$(this).index();
        bannerGalleryLoop(currentIndex);
        bannerGalleryInterval = setInterval(bannerGalleryLoop,5000);
    });

    $(".banner_bg li").hover(function(){
        $(".banner_bg li").stop(true,true);
        $(".thumbs li").stop(true,true);
        clearInterval(bannerGalleryInterval);
    },function(){
        bannerGalleryInterval = setInterval(bannerGalleryLoop,5000);
    });
});

function scoreChange(s){
    var score = parseInt(s)*360/100;
    var doughnutChartData = [
        {
            value: score,
            color:"#00A9E0"
        },
        {
            value : 360-score,
            color : "#ffffff"
        }
    ];

    var globalGraphSettings = {
        percentageInnerCutout : 80,
        segmentShowStroke : false,
        animationEasing : "easeOutQuart"
    };
}
