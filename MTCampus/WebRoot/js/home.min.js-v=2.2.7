        $(this).find("div").removeClass("div_active");
    },function(){
        $(this).find("img").removeClass("img_active");
        $(this).find("div").addClass("div_active");
    });
}else{
    //恢复旧效果
    $box.find("div").css({
        "opacity":"1"
    });
